<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="name_family" default="mecanum"/>
    <arg name="name_actuator" default="w1"/>

    <node pkg="hebi_okblab" exec="command_velocity" name="command_velocity" output="screen">
        <param name="name_family" value="$(var name_family)"/>
        <param name="name_actuator" value="$(var name_actuator)"/>
    </node>
</launch>